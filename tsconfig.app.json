{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue","src/**/*.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noImplicitAny": false,
    "allowJs": true,
    "module": "es2022",
    "moduleResolution": "node",
    "target": "es2022",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types":[
      "node"
    ]
  }
}
